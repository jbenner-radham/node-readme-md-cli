    "test": "c8 jasmine --config=test/support/jasmine.json"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "camelcase-keys": "^9.1.3",
    "chalk": "^5.3.0",
    "execa": "^9.4.0",
    "is-file-in-cwd": "^0.1.0",
    "js-yaml": "^4.1.0",
    "lodash.isplainobject": "^4.0.6",
    "log-symbols": "^7.0.0",
    "meow": "^13.2.0",
    "readme-md": "^1.2.0",
    "yarn-lockfile-exists": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.7.4",
    "c8": "^10.1.2",
    "command-exists": "^1.2.9",
    "eslint": "^9.12.0",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "jasmine": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "lint-staged": "^15.2.10",
    "sort-package-json": "^2.10.1",
    "tempy": "^3.1.0"
  },
  "engines": {
    "node": "^18.19.0 || >=20.5.0",
    "yarn": "1.x"
  },
  "preferGlobal": true
