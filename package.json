{
  "name": "readme-md-cli",
  "version": "0.12.0",
  "description": "Automatically generate a readme for your project from the CLI.",
  "keywords": [
    "cli",
    "commonmark",
    "docs",
    "documentation",
    "generator",
    "markdown",
    "md",
    "readme"
  ],
  "homepage": "https://github.com/jbenner-radham/node-readme-md-cli#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-readme-md-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-readme-md-cli.git"
  },
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "bin": {
    "readme-md": "./bin/cli.js"
  },
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "example": "example"
  },
  "scripts": {
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "prepare": "husky install",
    "readme": "./bin/cli.js > README.md",
    "test": "c8 jasmine"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "execa": "^6.1.0",
    "is-file-in-cwd": "^0.1.0",
    "js-yaml": "^3.12.2",
    "lodash.get": "^4.4.2",
    "lodash.result": "^4.5.2",
    "log-symbols": "^3.0.0",
    "make-dir": "^3.1.0",
    "meow": "^11.0.0",
    "readme-md": "^0.11.0",
    "yarn-lockfile-exists": "^0.2.0"
  },
  "devDependencies": {
    "c8": "^7.12.0",
    "command-exists": "^1.2.9",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "jasmine": "^4.5.0",
    "lint-staged": "^13.1.0",
    "sort-package-json": "^2.1.0",
    "tempy": "^0.3.0"
  },
  "engines": {
    "node": ">=16",
    "yarn": "1.x"
  },
  "preferGlobal": true
}
